{
    "name": "webpack2_config",
    "version": "1.0.0",
    "description": "Initial webpack config for new projects",
    "repository": {
        "type": "git",
        "url": "git://github.com/jeggett/webpack-initial.git"
    },
    "main": "index.js",
    "scripts": {
        "precommit": "npm run lint-staged",
        "prepush": "npm run lint-staged",
        "start": "webpack-dev-server --env development",
        "build": "webpack --env production",
        "stats": "webpack --env production --profile --json > stats.json",
        "test:lint": "eslint . --cache --format 'node_modules/eslint-friendly-formatter'",
        "flow-coverage": "flow-coverage-report -i 'app/**/*.js' -t html -t json -t text",
        "lint-staged": "lint-staged"
    },
    "lint-staged": {
        "*.js": "eslint --cache"
    },
    "keywords": [
        "webpack"
    ],
    "author": "Alexander Zaytsev",
    "license": "ISC",
    "devDependencies": {
        "babel-core": "^6.22.1",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^6.2.10",
        "babel-plugin-syntax-class-properties": "^6.13.0",
        "babel-plugin-syntax-decorators": "^6.13.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-syntax-flow": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.22.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-preset-env": "^1.1.8",
        "babel-preset-react": "^6.22.0",
        "babel-preset-react-hmre": "^1.1.1",
        "case-sensitive-paths-webpack-plugin": "^1.1.4",
        "clean-webpack-plugin": "^0.1.15",
        "css-loader": "^0.26.1",
        "eslint": "^3.14.1",
        "eslint-config-airbnb": "^14.0.0",
        "eslint-friendly-formatter": "^2.0.7",
        "eslint-loader": "^1.6.1",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "^3.0.2",
        "eslint-plugin-react": "^6.8.0",
        "extract-text-webpack-plugin": "^2.0.0-rc.2",
        "file-loader": "^0.10.0",
        "flow-bin": "^0.38.0",
        "flow-coverage-report": "^0.3.0",
        "font-awesome": "^4.7.0",
        "friendly-errors-webpack-plugin": "^1.1.3",
        "html-webpack-plugin": "^2.28.0",
        "html-webpack-template": "^6.0.1",
        "husky": "^0.13.1",
        "image-webpack-loader": "^3.2.0",
        "purifycss-webpack": "^0.4.2",
        "lint-staged": "^3.3.0",
        "node-sass": "^4.4.0",
        "postcss-loader": "^1.2.2",
        "sass-loader": "^4.1.1",
        "style-loader": "^0.13.1",
        "stylelint": "^7.7.1",
        "stylelint-scss": "^1.4.1",
        "url-loader": "^0.5.7",
        "webpack": "^2.2.1",
        "webpack-dev-server": "^2.2.0",
        "webpack-merge": "^2.6.1"
    },
    "dependencies": {
        "core-js": "^2.4.1",
        "purecss": "^0.6.2",
        "react": "^15.4.2",
        "react-dom": "^15.4.2",
        "react-redux": "^5.0.2",
        "redux": "^3.6.0"
    }
}
